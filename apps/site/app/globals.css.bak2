    padding-block: 48px;
  }

  .section--hero {
    padding-block: 72px;
  }

  .section--medium {
    padding-block: 56px;
  }

  .section--small {
    padding-block: 40px;
  }

  .section--logos {
    padding-block: 32px;
  }
}

/* =========================================================
   HELSINGBUSS - SEKTIONAVSTÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦ND (GLOBALT)
   =========================================================
   ÃƒÆ’Ã‚Â¢Ãƒâ€¦Ã¢â‚¬Å“ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦ HÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã…Â¾R ÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã…Â¾NDRAR DU AVSTÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦NDET mellan alla sektioner:
      Byt siffran pÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¥ --hb-section-gap (t.ex. 50px, 64px, 75px)

   Exempel:
     --hb-section-gap: 50px;  (tajt)
     --hb-section-gap: 64px;  (lagom)
     --hb-section-gap: 75px;  (luftigare)
   ========================================================= */

/* HB_SECTION_GAPS_START */
:root{
  /* ÃƒÆ’Ã‚Â¢Ãƒâ€¦Ã¢â‚¬Å“ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦ÃƒÆ’Ã‚Â¢Ãƒâ€¦Ã¢â‚¬Å“ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦ÃƒÆ’Ã‚Â¢Ãƒâ€¦Ã¢â‚¬Å“ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦ HÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã…Â¾R ÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã…Â¾NDRAR DU AVSTÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦NDET ÃƒÆ’Ã‚Â¢Ãƒâ€¦Ã¢â‚¬Å“ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦ÃƒÆ’Ã‚Â¢Ãƒâ€¦Ã¢â‚¬Å“ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦ÃƒÆ’Ã‚Â¢Ãƒâ€¦Ã¢â‚¬Å“ÃƒÂ¢Ã¢â€šÂ¬Ã‚Â¦ */
  --hb-section-gap: 30px;

  /* (valfritt) Hero-sektioner kan ha eget avstÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¥nd */
  --hb-section-gap-hero: 64px;
}

/* Wrapper fÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¶r vanliga sektioner */
.section{
  padding-top: var(--hb-section-gap);
  padding-bottom: var(--hb-section-gap);
}

/* Wrapper fÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¶r HERO (kant-till-kant) */
.section--hero{
  padding-top: var(--hb-section-gap-hero);
  padding-bottom: var(--hb-section-gap-hero);
}

/* Om du vill kunna slÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¥ av gap pÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¥ en specifik sektion */
.section--tight{
  padding-top: 0;
  padding-bottom: 0;
}
/* HB_SECTION_GAPS_END */


/* ================================
   HB HERO START
   ================================ */

/* ÃƒÆ’Ã‚Â°Ãƒâ€¦Ã‚Â¸ÃƒÂ¢Ã¢â€šÂ¬Ã‚ÂÃƒâ€šÃ‚Â½ÃƒÆ’Ã‚Â°Ãƒâ€¦Ã‚Â¸ÃƒÂ¢Ã¢â€šÂ¬Ã‚ÂÃƒâ€šÃ‚Â½ÃƒÆ’Ã‚Â°Ãƒâ€¦Ã‚Â¸ÃƒÂ¢Ã¢â€šÂ¬Ã‚ÂÃƒâ€šÃ‚Â½ HÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã…Â¾R ÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã…Â¾NDRAR DU HÃƒÆ’Ã†â€™ÃƒÂ¢Ã¢â€šÂ¬Ã¢â‚¬Å“JDEN ÃƒÆ’Ã‚Â°Ãƒâ€¦Ã‚Â¸ÃƒÂ¢Ã¢â€šÂ¬Ã‚ÂÃƒâ€šÃ‚Â½ÃƒÆ’Ã‚Â°Ãƒâ€¦Ã‚Â¸ÃƒÂ¢Ã¢â€šÂ¬Ã‚ÂÃƒâ€šÃ‚Â½ÃƒÆ’Ã‚Â°Ãƒâ€¦Ã‚Â¸ÃƒÂ¢Ã¢â€šÂ¬Ã‚ÂÃƒâ€šÃ‚Â½
   Exempel:
   520px  = lÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¥g hero
   600px  = lagom (REKOMMENDERAD)
   680px  = hÃƒÆ’Ã†â€™Ãƒâ€šÃ‚Â¶g
*/
:root {
  --hb-hero-height: 500px;
}

.section--hero {
  min-height: var(--hb-hero-height);
  padding-top: 64px;
  padding-bottom: 64px;
  display: flex;
  align-items: center;
}

/* ================================
   HB HERO END
   ================================ */

/* ================================
   HB SECTION SPACING START
   ================================ */

/* ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ HÃƒÆ’Ã¢â‚¬Å¾R ÃƒÆ’Ã¢â‚¬Å¾NDRAR DU AVSTÃƒÆ’Ã¢â‚¬Â¦NDET (DESKTOP) ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ */
:root{
  --hb-section-gap: 15px; /* desktop */
}

/* ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ HÃƒÆ’Ã¢â‚¬Å¾R ÃƒÆ’Ã¢â‚¬Å¾NDRAR DU AVSTÃƒÆ’Ã¢â‚¬Â¦NDET (MOBIL) ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ÃƒÂ°Ã…Â¸Ã¢â‚¬ÂÃ‚Â½ */
@media (max-width: 768px){
  :root{
    --hb-section-gap: 15px; /* mobil */
  }
}

/* Denna klass ska sitta pÃƒÆ’Ã‚Â¥ varje wrapper runt sektioner */
.section{
  padding-top: var(--hb-section-gap);
  padding-bottom: var(--hb-section-gap);
}

/* ================================
   HB SECTION SPACING END
   ================================ */

/* HB-BUTTONS START */
/* HÃƒâ€žR STYR DU ALLA KNAPPAR (samma fÃƒÂ¤rg + samma form ÃƒÂ¶verallt) */
:root{
  --hb-btn-bg: #0f5b56;        /* primÃƒÂ¤r knapp */
  --hb-btn-bg-hover: #0c4c48;  /* hover */
  --hb-btn-text: #ffffff;
  --hb-btn-radius: 999px;      /* pill som dina andra */
  --hb-btn-shadow: 0 10px 22px rgba(0,0,0,.18);
  --hb-btn-shadow-hover: 0 14px 28px rgba(0,0,0,.22);
}

.hb-btn{
  display: inline-flex;
  align-items: center;
  justify-content: center;
  gap: 10px;
  padding: 12px 22px;
  border-radius: var(--hb-btn-radius);
  font-weight: 700;
  line-height: 1;
  border: 0;
  cursor: pointer;
  text-decoration: none;
  transition: transform .15s ease, box-shadow .15s ease, background-color .15s ease, opacity .15s ease;
}

.hb-btn--primary{
  background: var(--hb-btn-bg);
  color: var(--hb-btn-text);
  box-shadow: var(--hb-btn-shadow);
}

.hb-btn--primary:hover{
  background: var(--hb-btn-bg-hover);
  box-shadow: var(--hb-btn-shadow-hover);
  transform: translateY(-1px);
}

.hb-btn--primary:active{
  transform: translateY(0px);
  opacity: .95;
}
/* HB-BUTTONS END */
/* ======================================================
   HÃ„R Ã„NDRAR DU AVSTÃ…NDET (mellan sektioner)
   Desktop: --hb-section-gap-desktop
   Mobil:   --hb-section-gap-mobile
   ====================================================== */
:root{
  --hb-section-gap-desktop: 72px; /* <-- HÃ„R Ã„NDRAR DU AVSTÃ…NDET (desktop) */
  --hb-section-gap-mobile: 30px;  /* <-- HÃ„R Ã„NDRAR DU AVSTÃ…NDET (mobil) */
}

.section{
  padding-top: var(--hb-section-gap-desktop);
}

@media (max-width: 768px){
  .section{
    padding-top: var(--hb-section-gap-mobile);
  }
}


