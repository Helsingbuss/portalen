.hb-fleet__content{
  position: relative;
  z-index: 1;
  max-width: 1320px;
  margin: 0 auto;
  padding: 0 24px;
  color: #fff;
}

.hb-fleet__title{
  margin: 0 0 10px;
  font-size: clamp(28px, 3.2vw, 44px);
  line-height: 1.08;
  letter-spacing: -0.02em;
  text-shadow: 0 10px 30px rgba(0,0,0,.35);
}

.hb-fleet__text{
  margin: 0 0 18px;
  max-width: 640px;
  font-size: 15.5px;
  line-height: 1.55;
  color: rgba(255,255,255,.88);
  text-shadow: 0 10px 30px rgba(0,0,0,.35);
