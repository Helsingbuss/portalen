  align-items: center;
  justify-content: center;
  background: rgba(255,255,255,.14);
  border: 1px solid rgba(255,255,255,.16);
  flex: 0 0 auto;
}
.hb-fleet__check svg{ width: 16px; height: 16px; }

.hb-fleet__btn{
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 12px 18px;
  border-radius: 999px;
  text-decoration: none;
  font-weight: 600;
  font-size: 14px;
  color: #fff;
  background: rgba(17, 72, 70, .95);
  border: 1px solid rgba(255,255,255,.14);
  box-shadow: 0 18px 40px rgba(0,0,0,.28);
  transition: transform .15s ease, filter .15s ease;
}
.hb-fleet__btn:hover{ transform: translateY(-1px); filter: brightness(1.04); }
.hb-fleet__btn:active{ transform: translateY(0px); filter: brightness(.98); }

.hb-fleet__logos{
  width: 100%;
  background: rgba(255,255,255,.82);
  border-top: 1px solid rgba(0,0,0,.06);
  border-bottom: 1px solid rgba(0,0,0,.06);
}

.hb-fleet__logosInner{
  max-width: 1320px;
  margin: 0 auto;
  padding: 18px 24px;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: clamp(18px, 4vw, 44px);
  flex-wrap: wrap;
}

.hb-fleet__logo{
  height: 32px;
  width: auto;
  opacity: .92;
  filter: grayscale(1);
}

@media (max-width: 900px){
  .hb-fleet__hero{ padding: 44px 0; min-height: 320px; }
  .hb-fleet__usp{ grid-template-columns: 1fr; max-width: 640px; }
  .hb-fleet__text{ font-size: 15px; }
  .hb-fleet__logo{ height: 28px; }
}

/* ===============================
   HB_FLEET_COMFORT_END
   =============================== */
 
/* HB_FLEET_LOGOS_TWEAK_START */
.hb-fleet__logos{
  display:flex;
  align-items:center;
  justify-content:flex-start;
  gap:28px;
  padding:14px 0 14px 22px;
  overflow-x:auto;
}
.hb-fleet__logos img{
