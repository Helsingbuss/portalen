  gap: 10px;
  padding: 10px 12px;
  border-radius: 14px;
  background: rgba(255,255,255,.07);
  border: 1px solid rgba(255,255,255,.10);
  backdrop-filter: blur(6px);
  box-shadow: 0 10px 30px rgba(0,0,0,.18);
  font-size: 13.5px;
  color: rgba(255,255,255,.92);
}

.hb-fleet__check{
